{
  "name": "swoft/annotation",
  "type": "library",
  "keywords": [
    "php",
    "swoole",
    "swoft"
  ],
  "description": "microservice framework base on swoole",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1",
    "doctrine/annotations": "^1.4",
    "php-di/phpdoc-reader": "^2.0"
  },
  "autoload": {
    "classmap": [
    ],
    "psr-4": {
      "Swoft\\Annotation\\": "src/"
    },
    "files": [
      "src/Helper/Functions.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Swoft\\Annotation\\Test\\": "test/"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.laravel-china.org"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml",
    "cs-fix": "./vendor/bin/php-cs-fixer fix $1"
  }
}
