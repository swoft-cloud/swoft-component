{
  "name": "swoft/annotation",
  "type": "library",
  "keywords": [
    "php",
    "swoole",
    "swoft"
  ],
  "description": "micro-service framework base on swoole",
  "license": "Apache-2.0",
  "require": {
    "php": ">7.1",
    "doctrine/annotations": "^1.4",
    "php-di/phpdoc-reader": "^2.0"
  },
  "autoload": {
    "classmap": [
    ],
    "psr-4": {
      "Swoft\\Annotation\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Annotation\\Unit\\": "test/unit/",
      "SwoftTest\\Annotation\\Testing\\": "test/testing/"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.laravel-china.org"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml",
    "cs-fix": "./vendor/bin/php-cs-fixer fix $1"
  }
}
