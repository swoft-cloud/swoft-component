{
  "name": "swoft/error",
  "type": "library",
  "keywords": [
    "php",
    "swoole",
    "swoft",
    "error-handler",
    "exception"
  ],
  "description": "Error Handler Component for Swoft",
  "license": "Apache-2.0",
  "require": {
    "swoft/stdlib": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\Error\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Error\\Unit\\": "test/unit",
      "SwoftTest\\Error\\Testing\\": "test/testing"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "swoft/framework": "~2.0.0"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
