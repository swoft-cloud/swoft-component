{
    "name": "swoft/memory",
    "type": "library",
    "keywords": [
        "php",
        "swoole",
        "swoft",
        "memory"
    ],
    "description": "Memory Component for Swoft",
    "license": "Apache-2.0",
    "require": {
        "swoft/framework": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Swoft\\Memory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SwoftTest\\Memory\\": "test/Cases"
        }
    },
    "require-dev": {
        "eaglewu/swoole-ide-helper": "dev-master",
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "^2.10"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit -c phpunit.xml",
        "cs-fix": "./vendor/bin/php-cs-fixer fix --config ./.php_cs $1"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    ]
}
