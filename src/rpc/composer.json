{
  "name": "swoft/rpc",
  "type": "library",
  "version": "v2.0.10",
  "keywords": [
    "php",
    "swoole",
    "swoft",
    "rpc"
  ],
  "description": "swoft rpc protocol component",
  "homepage": "https://github.com/swoft-cloud/swoft-rpc",
  "license": "Apache-2.0",
  "require": {
    "swoft/stdlib": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\Rpc\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Rpc\\Unit\\": "test/unit"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
